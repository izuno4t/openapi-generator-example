{
    "name": "openapi-generator-example",
    "version": "1.0.0",
    "description": "OpenAPI Example Specification",
    "main": "index.js",
    "scripts": {
        "ui": "browser-sync start -c ./config/browser-sync/bs-config-ui.js",
        "editor": "browser-sync start -c ./config/browser-sync/bs-config-editor.js",
        "static-html": "redoc-cli bundle ./openapi.yaml --output ./dist/static_html_spec/sample-spec.html",
        "lint": "speccy lint ./openapi.yaml"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/izuno4t/openapi-client-generator-example.git"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@openapitools/openapi-generator-cli": "2.5.2",
        "browser-sync": "^2.26.7",
        "cpx": "^1.5.0",
        "npm-run-all": "^4.1.5",
        "redoc-cli": "^0.9.7",
        "speccy": "^0.11.0"
    },
    "dependencies": {
        "js-yaml": "^4.0.0",
        "swagger-editor-dist": "^3.8.0",
        "swagger-ui-dist": "^3.25.0"
    }
}
